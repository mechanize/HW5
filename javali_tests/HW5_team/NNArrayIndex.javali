class Main {

    void swapfirst(int[] a1, int[] a2) {
        int temp;
        
        temp = a1[0];
        a1[0] = a2[0];
        a2[0] = temp;
    }


    void main() {

        int[] a1;
        int[] a2;

        a1 = new int[5];
        a2 = new int[5];
        a1[0] = 5;
        a2[0] = 2;
        swapfirst(a1, a2);
        write(a1[0]);
        writeln();
        write(a2[0]);
        writeln();
    }
}